{
    "version": 2, 
    "builds": [
        {
            "src": "src/main",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"]}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": ["GET", "PATCH", "POST", "PUT", "DELETE"]
        }
    ],
    "outputDirectory": "dist"
}